        android:supportsRtl="true"
        android:theme="@style/Theme.LaboratorioCalificado02">

        <!-- Actividad principal (MainActivity) -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Otras actividades -->
        <activity android:name=".Ejercicio01Activity" android:exported="false" />
        <activity android:name=".Ejercicio02Activity" android:exported="false" />
        <activity android:name=".DetalleActivity" android:exported="false" />
    </application>

</manifest>